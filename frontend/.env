# Base URL for the API (used by services/api.js). No trailing slash.
# Must include /api so that /login becomes .../api/login (matches Laravel route:list: POST api/login).
# Use the port your Laravel API runs on (e.g. 8000 with `php artisan serve`, or 8080 in Docker).
VITE_API_URL=http://localhost:3000/api

# OAuth: after provider sign-in, backend should redirect to this URL with ?token=... (&user=... optional).
# Default used by auth.service is: frontend origin + /login/callback (e.g. http://localhost:5173/login/callback)
