APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
# Frontend app URL (for password reset link in email, and CORS in dev)
FRONTEND_URL=http://localhost:5173

# Root domain for restaurant subdomains: [slug].RESTAURANT_DOMAIN (e.g. localhost â†’ pizza.localhost).
# Production: set to your domain (e.g. menus.example.com) and add wildcard DNS (*.menus.example.com).
RESTAURANT_DOMAIN=localhost
# CORS: comma-separated origins allowed to call the API (production). In local env, dev origins are allowed by default.
# CORS_ALLOWED_ORIGINS=https://app.example.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail: use "log" to write to storage/logs; use "smtp" + Mailhog to catch mail in dev.
# With Mailhog running (e.g. docker compose up mailhog), set MAIL_MAILER=smtp, MAIL_HOST=127.0.0.1, MAIL_PORT=1025.
# View received emails at http://localhost:8025
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Translation (LibreTranslate). Leave empty to use stub (no external calls).
# TRANSLATION_DRIVER=stub
# LIBRE_TRANSLATE_URL=https://libretranslate.com
# LIBRE_TRANSLATE_API_KEY=

# OAuth (for social login: Google, Facebook, Instagram)
# Frontend does the OAuth redirect; backend uses these to verify tokens.
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
FACEBOOK_CLIENT_ID=
FACEBOOK_CLIENT_SECRET=
INSTAGRAM_CLIENT_ID=
INSTAGRAM_CLIENT_SECRET=
